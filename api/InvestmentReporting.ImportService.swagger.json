{
  "openapi": "3.0.1",
  "info": {
    "title": "InvestmentReporting.ImportService",
    "version": "v1"
  },
  "paths": {
    "/Import": {
      "post": {
        "tags": [
          "Import"
        ],
        "parameters": [
          {
            "name": "date",
            "in": "query",
            "required": true,
            "schema": {
              "type": "string",
              "format": "date-time"
            }
          },
          {
            "name": "broker",
            "in": "query",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "requestBody": {
          "content": {
            "multipart/form-data": {
              "schema": {
                "required": [
                  "report"
                ],
                "type": "object",
                "properties": {
                  "report": {
                    "type": "string",
                    "format": "binary"
                  }
                }
              },
              "encoding": {
                "report": {
                  "style": "form"
                }
              }
            }
          }
        },
        "responses": {
          "201": {
            "description": "Success"
          },
          "400": {
            "description": "Bad Request"
          }
        }
      }
    }
  },
  "components": { }
}