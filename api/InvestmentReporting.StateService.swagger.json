{
  "openapi": "3.0.1",
  "info": {
    "title": "InvestmentReporting.StateService",
    "version": "v1"
  },
  "paths": {
    "/Broker": {
      "post": {
        "tags": [
          "Broker"
        ],
        "parameters": [
          {
            "name": "displayName",
            "in": "query",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "201": {
            "description": "Success"
          },
          "400": {
            "description": "Bad Request"
          }
        }
      }
    },
    "/State": {
      "get": {
        "tags": [
          "State"
        ],
        "responses": {
          "200": {
            "description": "Success",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/StateDto"
                }
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "BrokerDto": {
        "type": "object",
        "properties": {
          "displayName": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "StateDto": {
        "type": "object",
        "properties": {
          "brokers": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/BrokerDto"
            },
            "nullable": true
          }
        },
        "additionalProperties": false
      }
    }
  }
}